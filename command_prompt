cd ~                          # change directories to your home directory
mkdir version-control         # make version-control directory
cd version-control            # go to version-control directory
mkdir reflections             # create reflections directory
cd reflections                # go to reflections directory
subl lesson_1_reflections.txt # launch sublime with file called lesson_1_reflections.txt
ls -a                         # shows all the files (.git) in a repository

git clone <repository URL>    # clones a repository
git log                       # views commits made in that repository
git diff <commitID> <commitID> # shows the difference made between 2 commits
git checkout <commitID>       # looks into the commit or branch
git init                      # creates a new .git repository
git status                    # shows which files have changed since last commit
git add                       # adds changes to files into the staging area
git commit                    # makes a commit of the files in the staging area
git diff                      # shows difference between working directory and staging area
git diff --staged             # shows what commit is about to be made
git reset --hard              # removes changes made in the working directory and staging area